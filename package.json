{
  "name": "rego.fyi",
  "version": "0.0.2",
  "description": "Example of building a UI app and deploying in a single step with esbuild and CDK.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:website",
    "build:website": "NODE_ENV=production ts-node --files esbuild.ts build",
    "clean": "rimraf cdk.out coverage website/js",
    "deploy": "npm run clean && cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "lint": "eslint . --ext=.js,.ts",
    "pretest": "npm run lint",
    "start": "npm run clean && ts-node --files esbuild.ts",
    "synth": "cdk synth",
    "test": "npm run test:opa && npm run test:go && npm run test:ts",
    "test:opa": "opa test ./opa",
    "test:go": "go test ./fns/go",
    "test:ts": "jest --coverage"
  },
  "keywords": [
    "esbuild",
    "react",
    "cdk",
    "typescript"
  ],
  "author": "Matt Morgan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elthrasher/cdk-esbuild-s3-website"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.106.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/aws-lambda": "^8.10.76",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.17.1",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "aws-cdk": "1.106.1",
    "esbuild": "^0.12.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.3",
    "jest-raw-loader": "^1.0.1",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.106.1",
    "@aws-cdk/aws-certificatemanager": "1.106.1",
    "@aws-cdk/aws-cloudfront": "1.106.1",
    "@aws-cdk/aws-cloudfront-origins": "1.106.1",
    "@aws-cdk/aws-iam": "1.106.1",
    "@aws-cdk/aws-lambda": "1.106.1",
    "@aws-cdk/aws-lambda-nodejs": "1.106.1",
    "@aws-cdk/aws-logs": "1.106.1",
    "@aws-cdk/aws-route53": "1.106.1",
    "@aws-cdk/aws-route53-targets": "1.106.1",
    "@aws-cdk/aws-s3": "1.106.1",
    "@aws-cdk/aws-s3-deployment": "1.106.1",
    "@aws-cdk/core": "1.106.1",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
